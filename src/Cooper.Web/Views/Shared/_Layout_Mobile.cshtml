<!DOCTYPE html>
<html>
<head>
    <title>@ViewBag.Title @this.Suffix()</title>
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <link type="text/css" rel="Stylesheet" href="@Url.Content("~/content/jquery/css/mobile/jquery.mobile-1.1.0.min.css")" />
    <style type="text/css">
[class^="icon-"],
[class*=" icon-"] {
  display: inline-block;
  width: 14px;
  height: 14px;
  *margin-right: .3em;
  line-height: 14px;
  vertical-align: text-top;
  background-image: url("../content/bootstrap/img/glyphicons-halflings.png");
  background-position: 14px 14px;
  background-repeat: no-repeat;
}
    .icon-time {
  background-position: -48px -24px;
}
.icon-ok {
  background-position: -288px 0;
}
.icon-inbox {
  background-position: -168px -24px;
}
.icon-tag {
  background-position: 0 -48px;
}
    </style>
</head>
<body>
    @RenderBody()
    @{ Html.RenderSettings(); }
    <script type="text/javascript" src="@Url.Content("~/content/jquery/jquery.mobile-1.1.0.min.js")"></script>
    <script type="text/javascript" src="@Url.Content("~/scripts/mobile.js")"></script>
    <script type="text/javascript">
    </script>
    @RenderSection("Bottom", false)
</body>
</html>
